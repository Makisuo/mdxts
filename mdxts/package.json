{
  "name": "mdxts",
  "version": "0.1.5",
  "description": "Markdown by TypeScript",
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./components": "./dist/components/index.js",
    "./components/client": "./dist/components/client/index.js",
    "./rehype": "./dist/rehype/index.js",
    "./remark": "./dist/remark/index.js",
    "./next": "./dist/cjs/next/index.js",
    "./transform": "./dist/transform/index.js",
    "./utils": "./dist/utils/index.js",
    "./watcher": "./dist/watcher/index.js"
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "components": [
        "./dist/components/index.d.ts"
      ],
      "components/client": [
        "./dist/components/client/index.d.ts"
      ],
      "rehype": [
        "./dist/rehype/index.d.ts"
      ],
      "remark": [
        "./dist/remark/index.d.ts"
      ],
      "next": [
        "./dist/next/index.d.ts"
      ],
      "transform": [
        "./dist/transform/index.d.ts"
      ],
      "utils": [
        "./dist/utils/index.d.ts"
      ],
      "watcher": [
        "./dist/watcher/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "pnpm build:esm && pnpm build:cjs && pnpm build:types",
    "build:esm": "swc ./src -d ./dist --ignore **/*.test.ts",
    "build:cjs": "tsup",
    "build:types": "tsc",
    "dev": "pnpm build:esm --watch",
    "test": "jest"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "next": "14.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-stringify": "^10.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/mdx": "^2.3.0",
    "@next/mdx": "13.4.19",
    "@preact/signals-core": "^1.5.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.94",
    "@types/hast": "^3.0.0",
    "@types/mdast": "^4.0.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@types/webpack-env": "1.18.4",
    "@typescript/ata": "^0.9.4",
    "@typescript/vfs": "^1.5.0",
    "case-anything": "^2.1.13",
    "chokidar": "^3.5.3",
    "color": "^4.2.3",
    "copy-webpack-plugin": "^11.0.0",
    "esbuild": "^0.19.6",
    "estree-util-value-to-estree": "^3.0.1",
    "github-slugger": "^2.0.0",
    "hast-util-to-jsx-runtime": "^2.2.0",
    "hast-util-to-string": "^3.0.0",
    "mdast-util-to-string": "^4.0.0",
    "node-fetch": "^3.3.2",
    "rollup": "^4.1.5",
    "rollup-plugin-dts": "^6.1.0",
    "remark-gfm": "^4.0.0",
    "remark-typography": "^0.6.14",
    "remark-unwrap-images": "^4.0.0",
    "server-only": "0.0.1",
    "shiki": "^0.14.4",
    "title": "^3.5.3",
    "ts-morph": "^20.0.0",
    "unified": "^11.0.2",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-parents": "^6.0.1",
    "vfile": "^6.0.1",
    "vscode-oniguruma": "^1.7.0",
    "vscode-textmate": "^8.0.0"
  }
}
